{
  "name": "moviefinder",
  "version": "1.0.0",
  "description": "Movie gallery",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "prettier": "prettier --write src/**/*.{ts,tsx}",
    "lint:quiet": "eslint src/**/*.{ts,tsx} --quiet",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "lint": "eslint src/**/*.{ts,tsx}",
    "server": "json-server --watch db.json -m ./node_modules/json-server-auth -r routes.json --port 3070"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.nixdev.co/universejs/moviefinder.git"
  },
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=0.16"
  },
  "browser": {
    "[module-name]": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build:dev && npm run lint:quiet && npm run prettier"
    }
  },
  "devDependencies": {
    "@types/react-dom": "^17.0.9",
    "@types/react-input-mask": "^3.0.1",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^6.2.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "husky": "^7.0.1",
    "json-server": "^0.16.3",
    "json-server-auth": "^2.1.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "prettier": "^2.3.2",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.3",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@material-ui/styles": "^4.11.4",
    "@reduxjs/toolkit": "^1.6.1",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "formik": "^2.2.9",
    "html-webpack-plugin": "^5.3.2",
    "i": "^0.3.6",
    "i18next": "^20.6.1",
    "i18next-browser-languagedetector": "^6.1.2",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-devtools": "^4.14.0",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.4",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^7.0.4",
    "redux": "^4.1.0",
    "redux-persist": "^6.0.0",
    "use-debounce": "^7.0.0",
    "yup": "^0.32.9"
  }
}
